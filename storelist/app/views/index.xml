<Alloy>
	<Window class="container" title="Store List">
		<Button title="Refresh Stores" class="leftRefreshButton" onClick="storeManager.refreshAll"></Button>
        <Button title="Refresh US Stores" class="rightRefreshButton" onClick="storeManager.updateUSStores"></Button>
        <TableView dataCollection="store" class="displayTable">
            <TableViewRow title="{storeName}" />
        </TableView>
	</Window>
</Alloy>